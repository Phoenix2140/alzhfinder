<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoenix.alzhfinder">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/lanzador"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".PantallaPrincipal" />
        <activity android:name=".Pacientes" />
        <activity android:name=".Mapa" />
        <activity android:name=".InformacionPaciente" />
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" />
    </application>

</manifest>